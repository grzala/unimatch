<div class="create_society_form_or_whatever"> <!-- Jack, tag-naming is for you -->
    <h2>Create a society</h2>
    <%= form_for(:society, :url => {:controller => 'society', :action => 'create'}) do |f| %>
        <p> Name: </br> <%= f.text_field :name %></p>
        <p> Description: </br> <%= f.text_area :description, cols: 20, rows: 10 %> </p>
        
        <%= f.submit :Signup %>
    <% end %>
</div>
